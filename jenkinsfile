pipeline {
  agent none
     stages {     
       stage('Maven Install') {
             
     steps {
          sh 'mvn clean install'

          echo '***************************************************************'
        }
       }
       
    
       stage('Docker Build') {
         agent any
         steps {
           sh 'docker build -t priyesh/spring-petclinic:latest .'
         }
      }
    }
}